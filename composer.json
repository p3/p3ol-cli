{
    "name": "joecampo/retro-aol-cli",
    "description": "A P3OL CLI client.",
    "keywords": ["retro", "aol", "america online", "p3ol"],
    "homepage": "https://retroaol.org",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/joecampo/retro-aol-cli/issues",
        "source": "https://github.com/joecampo/retro-aol-cli"
    },
    "authors": [
        {
            "name": "Joe Campo",
            "email": "joe@discord.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "clue/stdio-react": "^2.6",
        "codedungeon/php-cli-colors": "^1.12",
        "illuminate/database": "^9.0",
        "illuminate/log": "^9.0",
        "laravel-zero/phar-updater": "^1.2",
        "lorisleiva/laravel-actions": "^2.4",
        "malios/php-to-ascii-table": "^3.0",
        "nunomaduro/laravel-console-menu": "^3.2",
        "nunomaduro/termwind": "^1.3",
        "rahul900day/laravel-console-spinner": "^1.1",
        "react/socket": "^1.11",
        "spatie/once": "^3.1"
    },
    "require-dev": {
        "clue/block-react": "^1.5",
        "fakerphp/faker": "^1.9.1",
        "laravel-zero/framework": "^9.0",
        "mockery/mockery": "^1.4.4",
        "pestphp/pest": "^1.21.1",
        "spatie/invade": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/p3ol"]
}
